@Article{Battisti2016,
  author   = {A. J. Battisti and D. Calzetti and R.-R. Chary},
  title    = {Characterizing Dust Attenuation in Local Star-forming Galaxies: UV and Optical Reddening},
  journal  = {The Astrophysical Journal},
  year     = {2016},
  volume   = {818},
  number   = {1},
  pages    = {13},
  abstract = {The dust attenuation for a sample of ∼10,000 local ( z ≲ 0.1) star-forming galaxies is constrained as a function of their physical properties. We utilize aperture-matched multiwavelength data available from the Galaxy Evolution Explorer and the Sloan Digital Sky Survey to ensure that regions of comparable size in each galaxy are being analyzed. We follow the method of Calzetti et al. and characterize the dust attenuation through the UV power-law index, β , and the dust optical depth, which is quantified using the difference in Balmer emission line optical depth, ##IMG## [http://ej.iop.org/images/0004-637X/818/1/13/apj522358ieqn1.gif] {${\tau }_{B}^{l}={\tau }_{{\rm{H}}\beta }-{\tau }_{{\rm{H}}\alpha }$} . The observed linear relationship between β and ##IMG## [http://ej.iop.org/images/0004-637X/818/1/13/apj522358ieqn2.gif] {${\tau }_{B}^{l}$} is similar to the local starburst relation, but the large scatter ( σ int = 0.44) suggests that there is significant variation in the local universe. We derive a selective attenuation curve over the range 1250Å < λ < 8320 Å and find that a single attenuation curve is effective for characterizing the majority of galaxies in our sample. This curve has a slightly lower selective attenuation in the UV compared to previously determined curves. We do not see evidence to suggest that a 2175 Å feature is significant in the average attenuation curve. Significant positive correlations are seen between the amount of UV and optical reddening and galaxy metallicity, mass, star formation rate (SFR), and SFR surface density. This provides a potential tool for gauging attenuation where the stellar population is unresolved, such as athigh z .},
  groups   = {dust},
  url      = {http://stacks.iop.org/0004-637X/818/i=1/a=13},
}


  url           = {https://link.aps.org/doi/10.1103/PhysRevLett.119.161101},
}

@Article{Huertas-Company2015,
  author   = {M. Huertas-Company and P. G. Pérez-González and S. Mei and F. Shankar and M. Bernardi and E. Daddi and G. Barro and G. Cabrera-Vives and A. Cattaneo and P. Dimauro and R. Gravet},
  title    = {The Morphologies of Massive Galaxies from z ~ 3—Witnessing the Two Channels of Bulge Growth},
  journal  = {The Astrophysical Journal},
  year     = {2015},
  volume   = {809},
  number   = {1},
  pages    = {95},
  abstract = {We quantify the morphological evolution of ##IMG## [http://ej.iop.org/images/0004-637X/809/1/95/apj516975ieqn1.gif] {$z\sim 0$} massive galaxies ( ##IMG## [http://ej.iop.org/images/0004-637X/809/1/95/apj516975ieqn2.gif] {${M}_{*}/{M}_{\odot }\sim {10}^{11.2\pm 0.3}$} ) from ##IMG## [http://ej.iop.org/images/0004-637X/809/1/95/apj516975ieqn3.gif] {$z\sim 3$} in the 5 CANDELS fields. The progenitors are selected using abundance matching techniques to account for the mass growth. At ##IMG## [http://ej.iop.org/images/0004-637X/809/1/95/apj516975ieqn4.gif] {$z\lt 1$} , the population matches the massive end of the Hubble sequence, with 30% of pure spheroids, 50% of galaxies with equally dominant disk and bulge components, and 20% of disks. At ##IMG## [http://ej.iop.org/images/0004-637X/809/1/95/apj516975ieqn5.gif] {$z\sim 2-3$} however, there is a majority of irregular systems ( ##IMG## [http://ej.iop.org/images/0004-637X/809/1/95/apj516975ieqn6.gif] {$\sim 60\%-70\%$} ) with still 30% of pure spheroids. We then analyze the stellar populations, star formation rates (SFRs), gas fractions, and structural properties for the different morphologies independently. Our results suggest two distinct channels for the growth of bulges in massive galaxies. Around ##IMG## [http://ej.iop.org/images/0004-637X/809/1/95/apj516975ieqn7.gif] {$\sim 30\%-40\%$} were already bulges at ##IMG## [http://ej.iop.org/images/0004-637X/809/1/95/apj516975ieqn8.gif] {$z\sim 2.5$} , with low average SFRs and gas fractions ( ##IMG## [http://ej.iop.org/images/0004-637X/809/1/95/apj516975ieqn9.gif] {$10\%-15\%$} ), high Sérsic indices ( ##IMG## [http://ej.iop.org/images/0004-637X/809/1/95/apj516975ieqn10.gif] {$n\gt 3-4$} ), and small effective radii ( ##IMG## [http://ej.iop.org/images/0004-637X/809/1/95/apj516975ieqn11.gif] {${R}_{{\rm{e}}}\sim 1$} kpc), pointing toward an even earlier formation through gas-rich mergers or violent disk instabilities. Between ##IMG## [http://ej.iop.org/images/0004-637X/809/1/95/apj516975ieqn12.gif] {$z\sim 2.5$} and ##IMG## [http://ej.iop.org/images/0004-637X/809/1/95/apj516975ieqn13.gif] {$z\sim 0$} , they rapidly increase their size by a factor of ##IMG## [http://ej.iop.org/images/0004-637X/809/1/95/apj516975ieqn14.gif] {$\sim 4-5$} , are quenched, and slightly increase their Sérsic indices ( ##IMG## [http://ej.iop.org/images/0004-637X/809/1/95/apj516975ieqn15.gif] {$n\sim 5$} ) but their global morphology remains unaltered. The structural evolution is independent of the gas fractions, suggesting that it is driven by ex situ events. The remaining 60% experience a gradual morphological transformation, from clumpy disks to more regular bulge+disk systems, essentially happening at ##IMG## [http://ej.iop.org/images/0004-637X/809/1/95/apj516975ieqn16.gif] {$z\gt 1$} . This results in the growth of a significant bulge component ( ##IMG## [http://ej.iop.org/images/0004-637X/809/1/95/apj516975ieqn17.gif] {$n\sim 3$} ) for 2/3 of the systems, possibly through the migration of clumps, while the remaining 1/3 retain a rather small bulge ( ##IMG## [http://ej.iop.org/images/0004-637X/809/1/95/apj516975ieqn20.gif] {$n\sim 1.5-2$} ). The transition phase between disturbed and relaxed systems and the emergence of the bulge is correlated with a decrease in the star formation activity and the gas fractions, suggesting a morphological quenching process as a plausible mechanism for the formation of these bulges.},
  groups   = {assemble/growth/size},
  url      = {http://stacks.iop.org/0004-637X/809/i=1/a=95},
}

@Article{Trujillo2014,
  author   = {Ignacio Trujillo and Anna Ferré-Mateu and Marc Balcells and Alexandre Vazdekis and Patricia Sánchez-Blázquez},
  title    = {NGC 1277: A Massive Compact Relic Galaxy in the Nearby Universe},
  journal  = {The Astrophysical Journal Letters},
  year     = {2014},
  volume   = {780},
  number   = {2},
  pages    = {L20},
  abstract = {As early as 10 Gyr ago, galaxies with more than 10 11 M ☉ of stars already existed. While most of these massive galaxies must have subsequently transformed through on-going star formation and mergers with other galaxies, a small fraction ( ##IMG## [http://ej.iop.org/icons/Entities/lsim.gif] {lsim} 0.1%) may have survived untouched until today. Searches for such relic galaxies, useful windows to explore the early universe, have been inconclusive to date: galaxies with masses and sizes like those observed at high redshift ( M ##IMG## [http://ej.iop.org/icons/Entities/sstarf.gif] {sstarf} ##IMG## [http://ej.iop.org/icons/Entities/gsim.gif] {gsim} 10 11 M ☉ ; R e ##IMG## [http://ej.iop.org/icons/Entities/lsim.gif] {lsim} 1.5 kpc) have been found in the local universe, but their stars are far too young for the galaxy to be a relic galaxy. This paper explores the first case of a nearby galaxy, NGC 1277 (at a distance of 73 Mpc in the Perseus galaxy cluster), which fulfills many criteria to be considered a relic galaxy. Using deep optical spectroscopy, we derive the star formation history along the structure of the galaxy: the stellar populations are uniformly old (>10 Gyr) with no evidence for more recent star formation episodes. The metallicity of their stars is super-solar ([Fe/H] = 0.20 ± 0.04 with a smooth decline toward the outer regions) and α-enriched ([α/Fe] = 0.4 ± 0.1). This suggests a very short formation time scale for the bulk of the stars in this galaxy. This object also rotates very fast ( V rot ~ 300 km s –1 ) and has a large central velocity dispersion (σ > 300 km s –1 ). NGC 1277 allows the exploration in full detail of properties such as the structure, internal dynamics, metallicity, and initial mass function as they were at ~10-12 Gyr ago when the first massive galaxies were built.},
  groups   = {nearby/CG},
  url      = {http://stacks.iop.org/2041-8205/780/i=2/a=L20},
}

@Article{Damjanov2015,
  author   = {Ivana Damjanov and Margaret J. Geller and H. Jabran Zahid and Ho Seong Hwang},
  title    = {Quiescent Compact Galaxies at Intermediate Redshift in the COSMOS Field. The Number Density},
  journal  = {The Astrophysical Journal},
  year     = {2015},
  volume   = {806},
  number   = {2},
  pages    = {158},
  abstract = {We investigate the evolution of compact galaxy number density over the redshift range ##IMG## [http://ej.iop.org/images/0004-637X/806/2/158/apj514223ieqn1.gif] {$0.2\lt z\lt 0.8$} . Our sample consists of galaxies with secure spectroscopic redshifts observed in the COSMOS field. With the large uncertainties, the compact galaxy number density trend with redshift is consistent with a constant value over the interval ##IMG## [http://ej.iop.org/images/0004-637X/806/2/158/apj514223ieqn2.gif] {$0.2\lt z\lt 0.8$} . Our number density estimates are similar to the estimates at ##IMG## [http://ej.iop.org/images/0004-637X/806/2/158/apj514223ieqn3.gif] {$z\gt 1$} for equivalently selected compact samples. Small variations in the abundance of the COSMOS compact sources as a function of redshift correspond to known structures in the field. The constancy of the compact galaxy number density is robust and insensitive to the compactness threshold or the stellar mass range (for ##IMG## [http://ej.iop.org/images/0004-637X/806/2/158/apj514223ieqn4.gif] {${M}_{*}\gt {10}^{10}\;{M}_{\odot }$} ). To maintain constant number density any size growth of high-redshift compact systems with decreasing redshift must be balanced by a formation of quiescent compact systems at ##IMG## [http://ej.iop.org/images/0004-637X/806/2/158/apj514223ieqn5.gif] {$z\lt 1$} .},
  groups   = {nearby/CG},
  url      = {http://stacks.iop.org/0004-637X/806/i=2/a=158},
}


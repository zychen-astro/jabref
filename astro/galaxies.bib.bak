@Article{Wuyts2011,
  author   = {Stijn Wuyts and Natascha M. Förster Schreiber and Dieter Lutz and Raanan Nordon and Stefano Berta and Bruno Altieri and Paola Andreani and Hervé Aussel and Angel Bongiovanni and Jordi Cepa and Andrea Cimatti and Emanuele Daddi and David Elbaz and Reinhard Genzel and Anton M. Koekemoer and Benjamin Magnelli and Roberto Maiolino and Elizabeth J. McGrath and Ana Pérez García and Albrecht Poglitsch and Paola Popesso and Francesca Pozzi and Miguel Sanchez-Portal and Eckhard Sturm and Linda Tacconi and Ivan Valtchanov},
  title    = {On Star Formation Rates and Star Formation Histories of Galaxies Out to z ~ 3},
  journal  = {The Astrophysical Journal},
  year     = {2011},
  volume   = {738},
  number   = {1},
  pages    = {106},
  abstract = {We compare multi-wavelength star formation rate (SFR) indicators out to z ~ 3 in the GOODS-South field. Our analysis uniquely combines U to 8 μm photometry from FIREWORKS, MIPS 24 μm and PACS 70, 100, and 160 μm photometry from the PEP, and H α spectroscopy from the SINS survey. We describe a set of conversions that lead to a continuity across SFR indicators. A luminosity-independent conversion from 24 μm to total infrared luminosity yields estimates of L IR that are in the median consistent with the L IR derived from PACS photometry, albeit with significant scatter. Dust correction methods perform well at low-to-intermediate levels of star formation. They fail to recover the total amount of star formation in systems with large SFR IR /SFR UV ratios, typically occuring at the highest SFRs (SFR UV + IR ##IMG## [http://ej.iop.org/icons/Entities/gsim.gif] {gsim} 100 M ☉ yr –1 ) and redshifts ( z ##IMG## [http://ej.iop.org/icons/Entities/gsim.gif] {gsim} 2.5) probed. Finally, we confirm that H α-based SFRs at 1.5 < z < 2.6 are consistent with SFR SED and SFR UV + IR provided extra attenuation toward H II regions is taken into account ( A V , neb = A V , continuum /0.44). With the cross-calibrated SFR indicators in hand, we perform a consistency check on the star formation histories inferred from spectral energy distribution (SED) modeling. We compare the observed SFR -M relations and mass functions at a range of redshifts to equivalents that are computed by evolving lower redshift galaxies backward in time. We find evidence for underestimated stellar ages when no stringent constraints on formation epoch are applied in SED modeling. We demonstrate how resolved SED modeling, or alternatively deep UV data, may help to overcome this bias. The age bias is most severe for galaxies with young stellar populations and reduces toward older systems. Finally, our analysis suggests that SFHs typically vary on timescales that are long (at least several 100 Myr) compared to the galaxies' dynamical time.},
  groups   = {SF/evolution},
  url      = {http://stacks.iop.org/0004-637X/738/i=1/a=106},
}

@Article{Whitaker2012,
  author   = {Katherine E. Whitaker and Pieter G. van Dokkum and Gabriel Brammer and Marijn Franx},
  title    = {The Star Formation Mass Sequence Out to z = 2.5},
  journal  = {The Astrophysical Journal Letters},
  year     = {2012},
  volume   = {754},
  number   = {2},
  pages    = {L29},
  abstract = {We study the star formation rate (SFR)-stellar mass ( M ##IMG## [http://ej.iop.org/icons/Entities/sstarf.gif] {sstarf} ) relation in a self-consistent manner from 0 < z < 2.5 with 22,816 star-forming galaxies selected from the NEWFIRM Medium-Band Survey. We find a significant nonlinear slope of the relation, SFR ##IMG## [http://ej.iop.org/icons/Entities/vprop.gif] {vprop} M 0.6 ##IMG## [http://ej.iop.org/icons/Entities/sstarf.gif] {sstarf} , and a constant observed scatter of 0.34 dex, independent of redshift and M ##IMG## [http://ej.iop.org/icons/Entities/sstarf.gif] {sstarf} . However, if we select only blue galaxies we find a linear relation SFR ##IMG## [http://ej.iop.org/icons/Entities/vprop.gif] {vprop} M ##IMG## [http://ej.iop.org/icons/Entities/sstarf.gif] {sstarf} , similar to previous results at z = 0 by Peng et al. This selection excludes red, dusty, star-forming galaxies with higher masses, which brings down the slope. By selecting on L IR / L UV (a proxy for dust obscuration) and the rest-frame U – V colors, we show that star-forming galaxies fall in three distinct regions of the log(SFR)-log( M ##IMG## [http://ej.iop.org/icons/Entities/sstarf.gif] {sstarf} ) plane: (1) actively star-forming galaxies with "normal" dust obscuration and associated colors (54% for log ( M ##IMG## [http://ej.iop.org/icons/Entities/sstarf.gif] {sstarf} ) > 10 at 1 < z < 1.5), (2) red star-forming galaxies with low levels of dust obscuration and low-specific SFRs (11%), and (3) dusty, blue star-forming galaxies with high-specific SFRs (7%). The remaining 28% comprises quiescent galaxies. Galaxies on the "normal" star formation sequence show strong trends of increasing dust attenuation with stellar mass and a decreasing specific SFR, with an observed scatter of 0.25 dex. The dusty, blue galaxies reside in the upper envelope of the star formation sequence with remarkably similar spectral shapes at all masses, suggesting that the same physical process is dominating the stellar light. The red, low-dust star-forming galaxies may be in the process of shutting off and migrating to the quiescent population.},
  groups   = {SF/evolution},
  url      = {http://stacks.iop.org/2041-8205/754/i=2/a=L29},
}


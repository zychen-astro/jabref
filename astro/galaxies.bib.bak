@Article{Izotov2016,
  author  = {Izotov, Y. I. and Schaerer, D. and Thuan, T. X. and Worseck, G. and Guseva, N. G. and Orlitová, I. and Verhamme, A.},
  title   = {Detection of high Lyman continuum leakage from four low-redshift compact star-forming galaxies},
  journal = {Monthly Notices of the Royal Astronomical Society},
